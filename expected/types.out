create type myenum as enum ('TRUE', 'FALSE', 'FILE_NOT_FOUND');
create function pg_temp.f1(a myenum) returns text language pllua as $$ print(a,type(a)) return a $$;
select pg_temp.f1(x) from unnest(enum_range(null::myenum)) x;
